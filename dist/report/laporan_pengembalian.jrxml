	<queryString>
		<![CDATA[SELECT peminjaman.*, member.nama, member.kelas, DATE_FORMAT(tanggal, '%d/%m/%Y') as tanggal_pinjam
FROM peminjaman
JOIN member ON member.id=peminjaman.id_member
WHERE peminjaman.tanggal_kembali >= $P{from} AND peminjaman.tanggal_kembali <= $P{until}
AND peminjaman.status='kembali'
ORDER BY peminjaman.tanggal_kembali ASC;]]>
	</queryString>
